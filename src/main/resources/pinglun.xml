<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pinglun">
<select id="selectpinglun" resultType="org.example.dogs">
    SELECT * FROM comments;
</select>
<insert id="insertpinglun" parameterType="org.example.dogs">
    insert into comments(name,view) values(#{name},#{view});
</insert>
</mapper>